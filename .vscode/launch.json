{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Flyfox AI Backend",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/flyfox_complete_launch.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Quantum Voice Assistant",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/quantum_voice_assistant_simple.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Flyfox Payment Integration",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/flyfox_payment_integration.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Dynex Quantum Integration",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/flyfox_dynex_nuco_integration.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "React Frontend (Vite)",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/flyfox-ai-platform/node_modules/.bin/vite",
            "args": ["--port", "3000"],
            "cwd": "${workspaceFolder}/flyfox-ai-platform",
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development"
            }
        },
        {
            "name": "Test Build",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test_build.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Advanced Dynex Examples",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/advanced_dynex_examples.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Quick Deploy",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/flyfox_quick_deploy.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Customer Management",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/flyfox_customer_management.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}